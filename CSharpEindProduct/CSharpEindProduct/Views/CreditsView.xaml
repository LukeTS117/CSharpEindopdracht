<Page x:Class="CSharpEindProduct.Views.CreditsView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CSharpEindProduct.Views"
             mc:Ignorable="d" 
             d:DesignHeight="800" d:DesignWidth="1200">
    <Grid Margin="-6,0,6,0" >
        <Grid.Background>
            <ImageBrush ImageSource="/CSharpEindProduct;component/Resources/MenuBackground.jpg" Stretch="UniformToFill"/>
        </Grid.Background>
        <Image HorizontalAlignment="Center" Height="488" Margin="0" VerticalAlignment="Center" Width="610" Source="/CSharpEindProduct;component/Resources/credits.png">
            <Image.OpacityMask>
                <ImageBrush ImageSource="/CSharpEindProduct;component/Resources/credits.png"/>
            </Image.OpacityMask>
        </Image>
        <Image HorizontalAlignment="Left" Height="100" Margin="10,10,0,0" VerticalAlignment="Top" Width="100" Source="/CSharpEindProduct;component/Resources/Back.png" RenderTransformOrigin="0.544,0.072" MouseDown="Button_Back_Down"/>
    </Grid>
</Page>
